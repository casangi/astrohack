{
    "panel":{
        "image_name":{
            "type":["str"],
            "required": true
        },
        "panel_name":{
            "type":["str"],
            "nullable": true
        },
        "clip_type":{
            "type":["str"],
            "allowed": ["absolute", "sigma", "relative", "noise_threshold"],
            "nullable": false
        },
        "clip_level":{
            "type":["float", "int", "dict"],
            "min": 0,
            "nullable": false,
            "struct type": ["dict"]
        },
        "panel_model":{
            "type":["str"],
            "check allowed with": "panel.models"
        },
        "panel_margins":{
            "type":["float"],
            "min": 0,
            "max": 0.5
        },
        "polarization_state":{
            "type": ["string"],
            "check allowed with": "panel.pol_states"
        },
        "ant":{
            "type":["list", "str"],
            "struct type": ["str"],
            "nullable": false
        },
        "ddi":{
            "type":["ndarray", "int", "list", "str"],
            "struct type": ["int", "str"],
            "nullable": false
        },
        "parallel":{
            "type":["boolean"]
        },
        "overwrite":{
            "type":["boolean"]
        }
    }
}
