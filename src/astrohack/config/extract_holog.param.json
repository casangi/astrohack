            "nullable": true
        },
        "holog_name":{
            "type":["str"],
            "nullable": true
        },
        "point_name":{
            "type":["str"]
        },
        "data_column":{
            "type":["str"],
            "allowed":["DATA", "CORRECTED_DATA"]
        },
        "time_smoothing_interval":{
            "type":["float"],
            "nullable": true,
